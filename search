    load("\033[1;91m[\033[1;93m!\033[1;91m]\033[1;95mLa recherche de tweets d'utilisateurs est en cours")
    print("\n")
    recherche = webbrowser.open_new("https://mobile.twitter.com/search?q="+_requete_en_cours+"")        
    load("\033[1;91m[\033[1;93m+\033[1;91m]\033[1;97m Recherche de l'utilisateur dans Google\033[1;92m...")
    print("\n")
    reche = webbrowser.open_new('http://www.google.com/search?q='+_requete_en_cours+'&pov=107128685182183119625&usg=__uAMtO0js_mAj03ni57W7YeWKuB8=&hl=ar')
    raw_input('\n\033[1;91m[\033[1;93m+\033[1;91m]\033[1;97m Appuyer entrer pour retourner au menu\033[1;92m...')
